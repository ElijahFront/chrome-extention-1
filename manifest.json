{
  "manifest_version": 2,

  "name": "Решу егэ cleaner",
  "description": "This extension turns off pop-ups at the ReshyEge website",
  "version": "2.3",
  "short_name":"ReshuEge cleaner",
  "author":"Ilya Smetanin",

  "browser_action": {
    "default_icon": "stop.png",
    "default_popup": "popup.html",
    "default_title": "Pop-ups info"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://math-ege.sdamgia.ru/*", "https://soc-ege.sdamgia.ru/*", "https://en-ege.sdamgia.ru/*", "https://mathb-ege.sdamgia.ru/*",
      "https://inf-ege.sdamgia.ru/*", "https://rus-ege.sdamgia.ru/*", "https://de-ege.sdamgia.ru/*", "https://fr-ege.sdamgia.ru/*", "https://sp-ege.sdamgia.ru/*",
      "https://phys-ege.sdamgia.ru/*", "https://chem-ege.sdamgia.ru/*", "https://bio-ege.sdamgia.ru/*", "https://geo-ege.sdamgia.ru/*",
      "https://lit-ege.sdamgia.ru/*", "https://hist-ege.sdamgia.ru/*", "https://ege.sdamgia.ru/*", "https://sdamgia.ru/*", "https://math-oge.sdamgia.ru/*",
    "https://bio-oge.sdamgia.ru/*", "https://rus-oge.sdamgia.ru/*", "https://en-oge.sdamgia.ru/*", "https://phys-oge.sdamgia.ru/*", "https://de-oge.sdamgia.ru/*",
  "https://chem-oge.sdamgia.ru/*", "https://fr-oge.sdamgia.ru/*", "https://lit-oge.sdamgia.ru/*", "https://sp-oge.sdamgia.ru/*", "https://inf-oge.sdamgia.ru/*",
"https://soc-oge.sdamgia.ru/*", "https://hist-oge.sdamgia.ru/*", "https://geo-oge.sdamgia.ru/*"],
      "js": ["delete.js"]
    }
  ]
}
